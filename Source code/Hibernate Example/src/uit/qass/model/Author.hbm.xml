<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="uit.qass.model">
  <class name="Author" table="dblp_author_ref_new">
    <id column="id" name="id" type="integer">
      <generator class="assigned"/>
    </id>
    <property column="author" name="author" type="string"/>
    <property column="editor" name="editor" type="boolean"/>
    <property column="author_num" name="authorNum" type="integer"/>
    <many-to-one class="Publication" column="id" insert="false" name="publications" update="false"/>
  </class>
</hibernate-mapping>
